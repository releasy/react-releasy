{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "target": "es2015",
    "module": "es2015",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["es2015", "dom"]
  }
}